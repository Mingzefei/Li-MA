@article{smithToolsAnalysisAccelerated2016,
  title = {Tools for {{Analysis}} of {{Accelerated Life}} and {{Degradation Test Data}}},
  author = {Smith, Reuel and Modarres, Mohammad},
  year = {2016},
  journal = {2016 Ieee Accelerated Stress Testing \& Reliability Conference (astr)},
  abstract = {This paper discusses educational tools, resources and experience in teaching physics-of-failure (PoF) modeling and data analysis methods used in accelerated life testing (ALT) and accelerated degradation testing (ADT). The concepts introduced in this paper include several probabilistic and highly technical approaches such as the PoF and mechanistic approaches to reliability prediction and assessments as well as advanced Bayesian analysis methods used in ALT and ADT to analyze life data obtained through various methods of constant and variable stress testing efforts. Other resources include codes and algorithms from the readily available computational tools R, MATLAB, and WinBUGS for solving advanced model and life estimation methods related to PoF, ALT and ADT analyses. This paper provides an overview of these resources and several example problems.},
  isbn = {978-1-5090-1880-2},
  annotation = {WOS:000390251600015}
