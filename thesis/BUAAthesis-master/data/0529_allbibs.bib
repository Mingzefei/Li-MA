@article{liDFMEAbasedReliabilityPrediction2018,
  title = {A {{DFMEA-based Reliability Prediction Approach}} in {{Early Product Design}}},
  author = {Li, Zhaojun and Mobin, Mohammadsadegh},
  year = {2018},
  journal = {2018 Annual Reliability and Maintainability Symposium (rams)},
  issn = {0149-144X},
  abstract = {In early product development stages, reliability growth testing (RGT) has been traditionally used for reliability monitoring and prediction through testing data. It has been criticized that the traditional reliability growth models are not only costly but also time consuming in terms of discovering and fixing potential failure modes. To tackle such challenges, accelerated reliability growth testing methods are adopted in industry. However, system level reliability growth testing can be still very challenging due to the stringent budget and tight product development cycle. On the other hand, reliability knowledge and information from the product development and design processes have not been well utilized for reliability prediction and management. These information include design simulation outcomes from finite element analysis (FEA) and computational fluid analysis (CFD), physics-of-failure (PoF) failure models and failure analysis from field failure events, and manufacturing defect reports. Such reliability information are usually included in the design failure mode and effect analysis (DFMEA) reports. This research explores a method for system reliability prediction by aggregating all the above mentioned reliability knowledge embedded in DFMEA. Specifically, we propose to utilize the failure frequency and detection assessments of the potential failure modes in DFMEA for system level reliability prediction through a set of Weibull models with three categories of failure rates. The three failure categories are modeled using Weibull models with unique shape parameters for each of the failure types. The characteristic life associated with each failure category is derived from the occurrence rate of the DFMEA outcomes. The component level failure information is further aggregated to the higher subsystem and system level reliability in a hierarchical way. In addition, failure detection assessments are incorporated into the Weibull models to monitor and predict the reliability growth over time. The proposed DFMEA-based reliability prediction method can well aggregate all the reliability knowledge from multiple design experts and increasingly adopted engineering analytical analysis. The traditional RGT outcomes can also be integrated into the proposed Weibull reliability prediction model. A numerical example shows the application of the proposed method for the reliability monitoring and prediction of a next generation engine design.},
  isbn = {978-1-5386-2869-0},
  keywords = {⛔ No DOI found},
  annotation = {ZSCC: 0000005  WOS:000617940600146}
}

