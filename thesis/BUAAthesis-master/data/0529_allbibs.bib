@phdthesis{liuxingboSnJingLiKuoSanGeXiangYiXingDuiCuSnNiXianXingHanDianDianQianYiXingWeiYingXiang2018,
  type = {硕士},
  title = {{{Sn晶粒扩散各向异性对Cu}}/{{Sn}}/{{Ni线性焊点电迁移行为影响}}},
  author = {{刘兴博} and {黄明亮}},
  year = {2018},
  abstract = {随着3D电子封装的高速发展,微焊点的尺寸急剧减小,使得微焊点中仅存在单个或者数个Sn晶粒。Sn原子在固态下以{$\beta$}-Sn体心四方结构存在,表现出显著的扩散各向异性,对微焊点电迁移行为有着强烈影响。Ni由于在回流过程和Sn有着较低的反应速率而被广泛应用于电子封装中。因此,亟待研究Sn晶粒取向对典型的Cu/Sn/Ni结构线性焊点的电迁移行为影响。本论文首先采用原位电迁移实验研究了Cu/Sn/Ni线性焊点在Ni作阴极时的固-固电迁移行为,并与Cu/SAC305/Cu线性焊点进行了对比。在研究中发现Ni基板溶解理论计算值与实验值相差较大,即阴极界面处(Cu,Ni)\_6Sn\_5对Ni原子电迁移扩散有阻碍作用。并利用EBSD排除Sn扩散各向异性对焊点电迁移行为的影响,通过不同时效工艺控制(Cu,Ni)\_6Sn\_5层厚度,对(Cu,Ni)\_6Sn\_5阻碍Ni原子电迁移扩散行为进行研究。本论文主要结论如下:(1)在通电时间相同条件下,Ni基板的溶解量随着\texttheta 角({$\beta$}-Sn晶粒的c轴与电子流动方向的夹角)的增大而减小,与cos\textasciitilde 2\texttheta 呈线性关系。并根据阴极界面处(Cu,Ni)\_6Sn\_5阻碍Ni原子电迁移扩散作用对Ni基板的溶解模型加以修正。(2)当电子由小\texttheta 角晶粒流向大\texttheta 角晶粒时,阴极侧Ni基板会大量溶解,大量(Ni,Cu)\_3Sn\_4型IMC会在小\texttheta 角晶粒中聚集析出,最终阴极过量溶解、阴极界面处开裂会造成焊点电迁移失效;当电子由大\texttheta 角晶粒流向小\texttheta 角晶粒时,阴极侧Ni基板极少溶解,在钎料内部无大量IMC聚集析出,阴极界面处有凹陷裂纹、微空洞等Sn的自扩散现象。(3)在相同Sn晶粒取向下,对比Ni、Cu不同基板作阴极时焊点的电迁移行为,发现两者在损伤失效模式上有较大差异,并基于Ni/Cu扩散通量比值、IMC聚集析出造成体积收缩等观点进行了解释说明。(4)从晶粒尺寸、晶界扩散通量、热力学稳定性角度解释了(Cu,Ni)\_6Sn\_5阻碍Ni原子电迁移扩散机制。具体为:Ni基板溶解主要受Ni原子在IMC中晶界扩散通量控制;IMC层厚度的增加将导致IMC晶粒尺寸的增大,进而导致晶界面积的减小,抑制Ni原子沿晶界扩散;(Cu,Ni)\_6Sn\_5随着Ni含量的增加,其热力学稳定性逐渐增强,电迁移过程中更难以溶解,抑制晶界扩散通量的增长,进一步阻碍Ni原子的扩散。},
  school = {大连理工大学},
  keywords = {/unread,电迁移;微焊点;各向异性;扩散;Cu/Sn/Ni;失效模式;(Cu;Ni)_6Sn_5}
}

@article{liVirtualQualificationAircraft2005,
  title = {Virtual Qualification of the Aircraft Computer Based on Physics-of-Failure Reliability Assessment Approach},
  author = {Li, Y. H. and Wang, H. and Fu, Y. and Xu, M. and Wu, J. and Osterman, M. and Pecht, M.},
  editor = {Cui, L. and Tsang, A. H. C.},
  year = {2005},
  journal = {Proceedings of the 4th International Conference on Quality \& Reliability},
  pages = {765--772},
  abstract = {This paper presents a reliability assessment approach for electronics using physics-of-failure (PoF) based virtual qualification. For this purpose, an aircraft computer, which is responsible for the aircraft mission management, was selected and evaluated. The whole process includes developing a model of the physical hardware, characterizing the anticipated operating conditions, performing load transformation, conducting failure analysis. The system hardware reliability was evaluated according to the virtual qualification results and design weak links were identified under the specified load conditions. Finally, a virtual redesign recommendation, as an example of how to improve reliability via virtual qualification, was conducted to meet the specified requirement. Some other findings were also presented in this paper. A physical accelerated stress test (AST) and accelerated life test (ALT) to verify the virtual qualification results are in process.},
