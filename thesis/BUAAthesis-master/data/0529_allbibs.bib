@article{liIntelligentAgentModeling2019,
  title = {An {{Intelligent Agent Modeling Method}} for {{System Reliability}} from {{Perspectives}} of {{Failure Mechanisms}}},
  author = {Li, Yaqiu and Pan, Guangze and Li, Qian and Wang, Chunhui and Li, Xiaobing},
  year = {2019},
  journal = {2019 Annual Reliability and Maintainability Symposium (rams 2019) - R \& M in the Second Machine Age - the Challenge of Cyber Physical Systems},
  issn = {0149-144X},
  abstract = {PoF-based modeling method are capable of bringing many influential factors into the reliability model. However, modeling for a complex dynamic system is still challenging due to the diversity of components and failure mechanisms, various types of correlation at each level of system hierarchy. In this paper, a new definition of agent model is defined within the system reliability modeling field, three classes of agents are assigned to entities at different level of system, from conditional factors to the whole system, considering the physical characteristics of agents' counterparts. Then, the learning method for agents updating are proposed to mimic the system evolution according to the degradation or failure process. Finally, taking the bearing under a correlative fatigue-wear mechanism as an example, the agent model is constructed and the reliability status of specimen is estimated by agent learning. Analysis of the results indicates that agent model of system reliability can help understanding the uncertainty and make evaluation more accurate. This paper mainly focuses on the modeling method, however, the way to improve the efficiency of evaluation is not included, which we plan to discuss further.},
