@article{aalAddressingWeakLinks2020,
  title = {Addressing {{Weak Links}} in {{Automotive Reliability}}},
  author = {Aal, Andreas and Braggin, Jennifer and Amade, Antoine and Puttock, Mark},
  year = {2020},
  journal = {2020 Ieee International Integrated Reliability Workshop (iirw)},
  pages = {90--93},
  issn = {1930-8841},
  abstract = {As we move into a more electrified and automated reality, the sustainability of functional safe and secure electronic systems is a major concern of automobile manufacturers. The complexity of high performance systems is not possible without the application of the latest semiconductor technology nodes. However, the effort to compensate for reliability and robustness gaps in those technologies via resilient design strategies, that are also compliant to cyber security IP and that address defect related attack vectors, creates challenges with respect to performance, size, power, security and cost (PSPSC). Technology generation dependent resilient design measures always relate to the physics-of-failure (PoF), making it evident that defectivity management in the leading edge domain is no longer simply a manufacturing concern, but rather an interdisciplinary task for design and process engineering teams, including process logistics. A process monitoring approach that effectively helps to reduce a) the number of critical defects and b) the number of defect types automatically reduces the resilient design effort for detection and compensation (circuit adaption). In this paper we will illustrate anonymized case studies and exemplary fab benchmarks to bridge the link between design and process engineering in order to infer best practices for automotive related semiconductor manufacturing. Following a systems engineering approach, the effort/cost for effective defectivity management including contamination control is weighted against effort/cost for resilient design and test strategies. It is therefore possible to create a framework that systematically identifies the optimization potential in the below defect detection threshold domain.},
  isbn = {978-1-7281-7058-9},
  annotation = {WOS:000659349800019}
}

