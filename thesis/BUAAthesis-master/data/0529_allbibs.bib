@article{struttProgressDesignReliability2003,
  title = {Progress towards a 'design for Reliability' Methodology for Automotive Components and Systems},
  author = {Strutt, J. E. and Hall, P. L. and Georgiou, S. and Locker, G. and Bevan, D. and {Macdonald-Bradley}, C. and Morris, J.},
  editor = {Strutt, J. E. and Hall, P. L.},
  year = {2003},
  journal = {Global Vehicle Reliability: Prediction and Optimization Techniques},
  pages = {5--24},
  abstract = {The increasingly competitive and global nature of the automotive industry will place further demands on vehicle and component manufacturers during the next decade. These include the requirement to deliver products of high reliability without additional costs. In addition suppliers frequently have a contractual obligation to provide formal demonstrations that the customer's reliability and maintainability requirements have been achieved. There is, therefore, an urgent need to develop a methodology for forecasting a product's reliability at the design stage. None of the reliability tools currently available can do this, since their numerical computations rely solely on historical hazard rate data of dubious validity. This Chapter outlines a design for reliability methodology, which is essentially a visual, integrated, flexible, and user-friendly knowledge management 'toolkit'. This contains a range of interlinked databases for storage, retrieval, and display of system data, and a series of software modules which permit the analysis of the failure logic of systems, and the quantitative evaluation of component and system reliabilities. These quantitative computations should be capable of handling both historical hazard rate data and the output of probabilistic physics-of-failure models in an integrated manner. The toolkit includes the capability of generating and solving Bayesian belief networks for complex, multi-input failure modes. Examples of the use of the toolkit are given, drawn from aspects of the design of TRW's electrically assisted steering system. . The capability to generate BBN models for failure logic incorporating multiple causes or influences. Examples of various aspects of the planned development of the Prescient toolbox include the following. . The development of a capability to incorporate three-dimensional finite element modelling, simulation, and visualization into PoF/BBN failure models. This will be done using the MATLAB/FEMLAB/SIMULINK software packages. . The achievement of fully automated data linking and transfer between different modules of the system (e.g. the PoF module, the BBN analyser, the finite element analysis package, and existing commercial reliability software packages). . Development of the design database into a tool capable of advising the designer on the suitability of particular materials for different environmental and usage conditions. . Further links will be provided to a wide range of relevant documentation, e.g. reliability requirements definitions, materials properties data, design reports, test and lessons learned data, etc.},
  isbn = {1-86058-368-7},
  keywords = {⛔ No DOI found},
  annotation = {WOS:000181655800001}
}

