  title = {Exploring {{Nature}} and {{Predicting Strength}} of {{Hydrogen Bonds}}: {{A Correlation Analysis Between Atoms-in-Molecules Descriptors}}, {{Binding Energies}}, and {{Energy Components}} of {{Symmetry-Adapted Perturbation Theory}}},
  shorttitle = {Exploring {{Nature}} and {{Predicting Strength}} of {{Hydrogen Bonds}}},
  author = {Emamian, Saeedreza and Lu, Tian and Kruse, Holger and Emamian, Hamidreza},
  year = {2019},
  journal = {Journal of Computational Chemistry},
  volume = {40},
  number = {32},
  pages = {2868--2881},
  issn = {1096-987X},
  abstract = {This work studies the underlying nature of H-bonds (HBs) of different types and strengths and tries to predict binding energies (BEs) based on the properties derived from wave function analysis. A total of 42 HB complexes constructed from 28 neutral and 14 charged monomers were considered. This set was designed to sample a wide range of HB strengths to obtain a complete view about HBs. BEs were derived with the accurate coupled cluster singles and doubles with perturbative triples correction (CCSD(T))(T) method and the physical components of the BE were investigated by symmetry-adapted perturbation theory (SAPT). Quantum theory of atoms-in-molecules (QTAIM) descriptors and other HB indices were calculated based on high-quality density functional theory wave functions. We propose a new and rigorous classification of H-bonds (HBs) based on the SAPT decomposition. Neutral complexes are either classified as ``very weak'' HBs with a BE {$\geq$} -2.5 kcal/mol that are mainly dominated by both dispersion and electrostatic interactions or as ``weak-to-medium'' HBs with a BE varying between -2.5 and -14.0 kcal/mol that are only dominated by electrostatic interactions. On the other hand, charged complexes are divided into ``medium'' HBs with a BE in the range of -11.0 to -15.0 kcal/mol, which are mainly dominated by electrostatic interactions, or into ``strong'' HBs whose BE is more negative than -15.0 kcal/mol, which are mainly dominated by electrostatic together with induction interactions. Among various explored correlations between BEs and wave function-based HB descriptors, a fairly satisfactory correlation was found for the electron density at the bond critical point (BCP; {$\rho$}BCP) of HBs. The fitted equation for neutral complexes is BE/kcal/mol = - 223.08 \texttimes{} {$\rho$}BCP/a. u. + 0.7423 with a mean absolute percentage error (MAPE) of 14.7\%, while that for charged complexes is BE/kcal/mol = - 332.34 \texttimes{} {$\rho$}BCP/a. u. - 1.0661 with a MAPE of 10.0\%. In practice, these equations may be used for a quick estimation of HB BEs, for example, for intramolecular HBs or large HB networks in biomolecules. \textcopyright{} 2019 Wiley Periodicals, Inc.},
  copyright = {\textcopyright{} 2019 Wiley Periodicals, Inc.},
  langid = {english},
  keywords = {binding energy,correlation analysis,coupled cluster,hydrogen bond,quantum theory of atoms-in-molecules,supermolecular approach,symmetry-adapted perturbation theory},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/jcc.26068},
  file = {C\:\\Users\\Hua\\Zotero\\storage\\YS9UNYXN\\Emamian 等。 - 2019 - Exploring Nature and Predicting Strength of Hydrog.pdf;C\:\\Users\\Hua\\Zotero\\storage\\5JI4I7DU\\jcc.html}
}

@article{evansFuturePackageReliability1999,
  title = {Future Package Reliability Testing},
  author = {Evans, T. C.},
  year = {1999},
  journal = {1999 International Conference on High Density Packaging and Mcms, Proceedings},
  volume = {3830},
  pages = {389--394},
  issn = {0277-786X},
  abstract = {Semiconductor packages have always been stress tested to determine their reliability. After stress tests are completed as part of a qualification plan, decisions are made of whether the package can provide thermal, electrical, and mechanical integrity for the Integrated Circuit (IC) contained within. As package technology matures, and competitive pressures increase on a customer's electronic products, the typical package reliability estimations and qualification plans are having an adverse impact on cost and availability of marry new package types. Estimations use acceleration factors that are Mot really understood and ignore system level effects. Overstressing causes invalid failure mechanisms for the actual end-use conditions. Reliability evaluations are not aligned with the properties of many new package types and their performance in a greater variety of electronic products. Application specific qualification testing is part of a proposed reliability methodology using standard test vehicle data and physical modeling to determine expected lifetime over realistic end use conditions, and is based on a Physics of Failure (POF) approach to provide more appropriate feats in less time.},
  isbn = {0-930815-57-2},
