@article{kharazmiHpVPINNsVariationalPhysicsinformed2021,
  title = {Hp-{{VPINNs}}: {{Variational}} Physics-Informed Neural Networks with Domain Decomposition},
  author = {Kharazmi, Ehsan and Zhang, Zhongqiang and Karniadakis, George E. M.},
  year = {2021},
  month = feb,
  journal = {Computer Methods in Applied Mechanics and Engineering},
  volume = {374},
  pages = {113547},
  issn = {0045-7825},
  abstract = {We formulate a general framework for hp-variational physics-informed neural networks (hp-VPINNs) based on the nonlinear approximation of shallow and deep neural networks and hp-refinement via domain decomposition and projection onto the space of high-order polynomials. The trial space is the space of neural network, which is defined globally over the entire computational domain, while the test space contains piecewise polynomials. Specifically in this study, the hp-refinement corresponds to a global approximation with a local learning algorithm that can efficiently localize the network parameter optimization. We demonstrate the advantages of hp-VPINNs in both accuracy and training cost for several numerical examples of function approximation and in solving differential equations. (C) 2020 Elsevier B.V. All rights reserved.},
  annotation = {WOS:000608741800007}
}

