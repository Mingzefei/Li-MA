@article{camacho-foreroElucidatingElectrolyteDecomposition2017,
  title = {Elucidating Electrolyte Decomposition under Electron-Rich Environments at the Lithium-Metal Anode},
  author = {{Camacho-Forero}, Luis E. and Balbuena, Perla B.},
  year = {2017},
  journal = {Physical Chemistry Chemical Physics},
