@article{alberIntegratingMachineLearning2019,
  title = {Integrating Machine Learning and Multiscale Modeling-Perspectives, Challenges, and Opportunities in the Biologica Biomedical, and Behavioral Sciences},
  author = {Alber, Mark and Tepole, Adrian Buganza and Cannon, William R. and De, Suvranu and {Dura-Bernal}, Salvador and Garikipati, Krishna and Karniadakis, George and Lytton, William W. and Perdikaris, Paris and Petzold, Linda and Kuhl, Ellen},
  year = {2019},
  month = nov,
  journal = {Npj Digital Medicine},
  volume = {2},
  pages = {115},
  issn = {2398-6352},
  abstract = {Fueled by breakthrough technology developments, the biological, biomedical, and behavioral sciences are now collecting more data than ever before. There is a critical need for time- and cost-efficient strategies to analyze and interpret these data to advance human health. The recent rise of machine learning as a powerful technique to integrate multimodality, multifidelity data, and reveal correlations between intertwined phenomena presents a special opportunity in this regard. However, machine learning alone ignores the fundamental laws of physics and can result in ill-posed problems or non-physical solutions. Multiscale modeling is a successful strategy to integrate multiscale, multiphysics data and uncover mechanisms that explain the emergence of function. However, multiscale modeling alone often fails to efficiently combine large datasets from different sources and different levels of resolution. Here we demonstrate that machine learning and multiscale modeling can naturally complement each other to create robust predictive models that integrate the underlying physics to manage ill-posed problems and explore massive design spaces. We review the current literature, highlight applications and opportunities, address open questions, and discuss potential challenges and limitations in four overarching topical areas: ordinary differential equations, partial differential equations, data-driven approaches, and theory-driven approaches. Towards these goals, we leverage expertise in applied mathematics, computer science, computational biology, biophysics, biomechanics, engineering mechanics, experimentation, and medicine. Our multidisciplinary perspective suggests that integrating machine learning and multiscale modeling can provide new insights into disease mechanisms, help identify new targets and treatment strategies, and inform decision making for the benefit of human health.},
  annotation = {WOS:000503006300001},
  file = {C\:\\Users\\Hua\\Zotero\\storage\\8QQKL5Y2\\Alber et al_2019_Integrating machine learning and multiscale modeling-perspectives, challenges,.pdf}
}

@article{aldridgeReliabilityGrowthUsing2018,
  title = {Reliability {{Growth Using Development Test Data}} and {{PoF Damage Models}}},
  author = {Aldridge, Dustin and Todd, Andrew and Solomon, Rajeev and Hodgson, James},
  year = {2018},
  journal = {2018 Annual Reliability and Maintainability Symposium (rams)},
  issn = {0149-144X},
  abstract = {This approach to tracking reliability growth data is derived from a need defined by the customer. The foundation for success in reliability assurance starts with the customer and supplier relationship. There needs to be cooperation between the customer and supplier to achieve common goals in meeting requirements through mutual respect and open communication in the proposal process to define a common strategy and appropriately fund that strategy. The adopted strategy integrates two reliability growth methods, Physics of Failure (PoF) damage models related to the service life environments and an accepted reliability growth model with agreed upon fix effectiveness factors (FEF) necessary for prediction. The growth processes are: 1) A layered Highly Accelerated Life Test (HALT) process to find weaknesses and probe operational limits without destroying expensive hardware, 2) An overall Test Analyze and Fix (TAAF) strategy based upon service life requirements. In addition to HALT and TAAF, this approach seeks to include the damage due to stresses imparted on hardware during development testing. Defining and measuring the service environment (manufacturing, screening, storage, periodic reassessment, rework, interim testing, and operation) along with PoF damage model parameters are critical to creating a jointly accepted accelerated reliability growth program. Acceleration factors are based off what is defined as one product life. Tracking the exposed stress on all development hardware, not just those used in HALT and TAAF, becomes an accounting issue. Keeping track of all operation, environmental conditions, hardware configurations, test results, and the systems to enable this tracking throughout development testing and subsequent qualification testing can be burdensome. The approach described here provides a frame work to collect data from all available test data sources, appropriately combine this data for equivalent life calculations, count and track failures, and generate outputs to be used in the reliability growth model. As a proof of concept, this framework has been implemented using Excel workbooks and Visual Basic for Application macros.},
  isbn = {978-1-5386-2869-0},
  keywords = {⛔ No DOI found},
  annotation = {WOS:000617940600118}
}

