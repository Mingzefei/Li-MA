@article{nduRiskInformedLifeTest2018,
  title = {A {{Risk-Informed Life Test Modeling Framework}} for {{Uncertainty Characterization}} and {{Life Estimation}}},
  author = {Ndu, Obi K. and Smith, Clayton},
  year = {2018},
  journal = {2018 Annual Reliability and Maintainability Symposium (rams)},
  issn = {0149-144X},
  abstract = {A framework for assessing and comparing the uncertainties associated with failure mechanisms at end-ofdesign-life of any long-duration mission systems would provide utility in determining its readiness for flight. With this in mind, the Radioisotope Power Systems (RPS) program office at NASA Glenn Research Center (NASA GRC) and The Johns Hopkins University Applied Physics Laboratory (JHU/APL, or APL) have adopted a risk-informed life-testing process to determine the target, nature, and extent of testing required for the demonstration of reliability and life of radioisotope power systems. This paper presents the methodology for implementing such a process through the combined use of physics-of-failure models (POF) and Bayesian inference techniques. We highlight the differences in traditional methods of reliability demonstration and a risk-informed approach to system testing, we then present a discussion of physics-based failure modeling, the Bayesian paradigm in risk and uncertainty assessment, we conclude with a simple example that captures the nature of input and output expected from a Risk-informed Life Test modeling (RILT) approach to system end-of-design-life (EODL) characterization.},
  isbn = {978-1-5386-2869-0},
  keywords = {⛔ No DOI found},
  annotation = {WOS:000617940600059}
}

