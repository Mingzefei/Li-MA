@article{liangResearchDesignMethodology2016,
  title = {Research on the {{Design Methodology}} of {{Accelerated Thermal Cycling Test}} for {{Train Electronic Assemblies Basing}} on {{PoF}}},
  author = {Liang, Ce and Yao, Jianwei and Su, Yutai},
  editor = {Zuo, M. J. and Xing, L. and Li, Z. and Tian, Z. and Miao, Q.},
  year = {2016},
  journal = {2016 Prognostics and System Health Management Conference (phm-Chengdu)},
  issn = {2166-5656},
  abstract = {Recently thermal cycling tests for train electronic assemblies have been widely applied to eliminating latent defects and improving the reliability of the electronics. Considering the cost and schedule, accelerate thermal cycling (ATC) tests are conducted instead of normal tests. In this paper, the design methodology of ATC tests for electronic assemblies is studied. Moreover, the procedure of test design is established basing on Physics of Failure (PoF). The thermal cycling condition feature matrix and the electronic assemblies feature matrix are defined. Using Engelmaier model, the electronic assemblies life feature matrix in both normal and accelerate temperature conditions are consequently calculated. Comparing the two life prediction results, ATC test scheme equivalent to the normal test is determined. What's more, the ATC test scheme for a train traction monitor parameter acquisition circuit is designed as a case study.},
  isbn = {978-1-5090-2778-1},
  annotation = {WOS:000393560200178}
}

