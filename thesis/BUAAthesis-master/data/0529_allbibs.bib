  file = {C\:\\Users\\Hua\\Zotero\\storage\\XYYJDPGG\\Ravi 等。 - 2017 - Origin of the Substitution Mechanism for the Bindi.pdf;C\:\\Users\\Hua\\Zotero\\storage\\JDFZXW6L\\acs.jpclett.html}
}

