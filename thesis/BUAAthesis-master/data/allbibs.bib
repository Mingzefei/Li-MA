@article{zhangStableLithiumMetal2019,
  title = {Stable Lithium Metal Anodes Enabled by Inorganic/Organic Double-Layered Alloy and Polymer Coating},
  author = {Zhang, Yuanjun and Wang, Guanyao and Tang, Liang and Wu, Jiajie and Guo, Bingkun and Zhu, Ming and Wu, Chao and Dou, Shi Xue and Wu, Minghong},
  year = {2019},
  journal = {Journal of Materials Chemistry A},
  volume = {7},
  number = {44},
