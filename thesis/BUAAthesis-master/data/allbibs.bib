  shorttitle = {Exploring {{Nature}} and {{Predicting Strength}} of {{Hydrogen Bonds}}},
  author = {Emamian, Saeedreza and Lu, Tian and Kruse, Holger and Emamian, Hamidreza},
  year = {2019},
  journal = {Journal of Computational Chemistry},
  volume = {40},
