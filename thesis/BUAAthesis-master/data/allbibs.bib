  author = {Chen, Xiang and Hou, Ting-Zheng and Li, Bo and Yan, Chong and Zhu, Lin and Guan, Chao and Cheng, Xin-Bing and Peng, Hong-Jie and Huang, Jia-Qi and Zhang, Qiang},
