@article{martinezdelahozReductionMechanismsAdditives2014,
  title = {Reduction Mechanisms of Additives on {{Si}} Anodes of {{Li-ion}} Batteries},
  author = {{Mart{\'i}nez de la Hoz}, Julibeth M. and Balbuena, Perla B.},
  year = {2014},
  journal = {Physical Chemistry Chemical Physics},
  volume = {16},
  number = {32},
