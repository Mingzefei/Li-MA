@article{zhaoMetallurgicallyLithiatedSiO2016,
  title = {Metallurgically Lithiated {{SiO}} {\textsubscript{x}} Anode with High Capacity and Ambient Air Compatibility},
  author = {Zhao, Jie and Lee, Hyun-Wook and Sun, Jie and Yan, Kai and Liu, Yayuan and Liu, Wei and Lu, Zhenda and Lin, Dingchang and Zhou, Guangmin and Cui, Yi},
