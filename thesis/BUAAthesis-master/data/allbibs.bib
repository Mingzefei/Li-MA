@article{alvaradoBisaltEtherElectrolytes2019,
  title = {Bisalt Ether Electrolytes: A Pathway towards Lithium Metal Batteries with {{Ni-rich}} Cathodes},
  shorttitle = {Bisalt Ether Electrolytes},
  author = {Alvarado, Judith and Schroeder, Marshall A. and Pollard, Travis P. and Wang, Xuefeng and Lee, Jungwoo Z. and Zhang, Minghao and Wynn, Thomas and Ding, Michael and Borodin, Oleg and Meng, Ying Shirley and Xu, Kang},
  year = {2019},
  journal = {Energy \& Environmental Science},
  volume = {12},
  number = {2},
  pages = {780--794},
  issn = {1754-5692, 1754-5706},
  doi = {10.1039/C8EE02601G},
  abstract = {Breakthroughs in performance of Li/Cu with Ni-rich cathodes can be achieved               via               manipulation of anion interfacial chemistry, as uncovered by experiment/modeling.                        ,                             The electrochemical performance and mechanistic effects of incorporating two salts in an ether electrolyte in Li\textendash metal cells were investigated experimentally and               via               molecular scale modeling. Improvements in efficiency and cycling stability over baseline electrolytes in lithium (Li)               versus               copper (Cu) cells were directly correlated to the initial Li-nucleation process, as observed               via               cryogenic-focused ion beam (cryo-FIB) prepared cross sections, which revealed that Li films deposited with the bisalt electrolyte were significantly thinner and denser than those from the baseline. This behavior was further traced back to the initial nucleation process               via               cryogenic transmission electron microscopy (cryo-TEM), which shows stark differences in the morphology and conformality of deposited Li as a function of electrolyte chemistry. X-ray photoelectron spectroscopy (XPS) indicated that the solid electrolyte interphase (SEI) formed from the bisalt electrolyte primarily consists of larger anion fragments, suggesting that the anion chemistry strongly influences the extent of reduction and resulting surface chemistry.               Ab initio               DFT-based and force field-based molecular dynamics calculations revealed the complex interplay (positioning, orientation, reactivity, kinetics) between the FSI               -               and TFSI               -               anions in the double layer at both electrode\textendash electrolyte interfaces and the ramifications for stabilizing these interfaces. As a result of the unique interphasial chemistry brought by this bisalt system, this electrolyte supports an unprecedented (for an ether-based electrolyte) capacity retention ({$>$}88\%) after 300 cycles ({$\sim$}2 months of cycling) in a 4.4 V NMC622-Li cell, and a {$>$}30\% improvement in capacity retention over baseline after 50 cycles in 4.4 V NMC622-Cu ``anode-free'' cells. Altogether, these results provide new insight into how the bisalt effect can be leveraged for regulating the timescale, chemistry, and extent of interfacial reactions. When balanced properly, this promotes efficient plating/deplating of Li, and potentially supports widespread implementation of high nickel content NMC cell configurations with limited or no excess lithium.},
  langid = {english},
  keywords = {重要},
  annotation = {ZSCC: 0000189},
  file = {C\:\\Users\\Hua\\Zotero\\storage\\R39DWW77\\Alvarado 等。 - 2019 - Bisalt ether electrolytes a pathway towards lithi.pdf;C\:\\Users\\Hua\\Zotero\\storage\\YI9ZDRSC\\A-179.pdf}
}

